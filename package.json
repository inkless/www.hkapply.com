{
  "name": "www.hkapply.com",
  "version": "0.1.0",
  "description": "website for hkapply",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:inkless/www.hkapply.com.git"
  },
  "keywords": [
    "apply",
    "hong",
    "kong",
    "university"
  ],
  "author": "Guangda Zhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkless/www.hkapply.com/issues"
  },
  "homepage": "http://www.hkapply.com",
  "scripts": {
    "clean": "rm -f static/dist",
    "icons": "sprity create static/src/images/ static/src/images/icons/*.png -s ../../src/styles/_sprite.scss -p sass --style-type scss -d 1:96 -d 2:192",
    "lint": "eslint static/src/js",
    "serve": "browser-sync start --proxy http://localhost:3005 --files 'static/dist/images/*.*, static/dist/*.css, static/dist/*.js, **/*.html, !node_modules/**/*.html'",
    "watch": "npm run lint && webpack --progress --config webpack.config.js --colors --watch",
    "dev": "npm-run-all -p watch start serve",
    "build": "npm run lint && webpack --progress --config webpack.config.js --colors",
    "build:prod": "npm run lint && webpack --progress --config webpack.config.production.js --colors",
    "start": "node ./index.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "browser-sync": "^2.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "sass-loader": "^3.1.2",
    "sprity": "^1.0.8",
    "sprity-cli": "^1.0.1",
    "sprity-sass": "^1.0.4",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.6.1",
    "url-loader": "^0.5.7",
    "npm-run-all": "^1.5.1"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "photoswipe": "^4.1.1",
    "swig": "^1.4.2"
  }
}
